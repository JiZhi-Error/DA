<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/a0"/>
            <solid android:color="#eeda9f"/>
            <stroke android:width="@dimen/a2" android:color="#eeda9f"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/a0"/>
            <solid android:color="#b29234"/>
            <stroke android:width="@dimen/a2" android:color="#b29234"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/a0"/>
            <solid android:color="#dfb741"/>
            <stroke android:width="@dimen/a2" android:color="#dfb741"/>
        </shape>
    </item>
</selector>
