<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#dadada" android:endColor="#dadada" android:angle="270" android:centerY="0.75" android:centerColor="#dadada"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#21b310" android:endColor="#21b310" android:angle="270" android:centerY="0.75" android:centerColor="#21b310"/>
            </shape>
        </clip>
    </item>
</layer-list>
