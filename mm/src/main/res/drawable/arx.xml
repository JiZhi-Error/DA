<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/a6a"/>
            <size android:height="@dimen/g" android:width="@dimen/g"/>
            <corners android:radius="@dimen/kb"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/a6b"/>
            <size android:height="@dimen/g" android:width="@dimen/g"/>
            <corners android:radius="@dimen/kb"/>
        </shape>
    </item>
</selector>
