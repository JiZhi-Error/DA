<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="@color/afz" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
        <Button android:id="@+id/djv" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="GetSessionId" style="@style/nk"/>
        <Button android:id="@+id/djp" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="GetChatUserInfo" style="@style/nk"/>
        <Button android:id="@+id/djm" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="GetBlackList" style="@style/nk"/>
        <Button android:id="@+id/hl7" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="SetBlackList" style="@style/nk"/>
        <Button android:id="@+id/hl8" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="设置黑名单关联微信游戏账号" style="@style/nk"/>
        <Button android:id="@+id/djo" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="GetChatSessionData" style="@style/nk"/>
        <Button android:id="@+id/bfw" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="会话" style="@style/nk"/>
        <Button android:id="@+id/b3e" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="清除DB" style="@style/nk"/>
        <Button android:id="@+id/ci7" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="创建很多conversation" style="@style/nk"/>
        <Button android:id="@+id/i4f" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="直接进入聊天" style="@style/nk"/>
        <Button android:id="@+id/bmd" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="删除最新的一条Conversation" style="@style/nk"/>
        <Button android:id="@+id/hle" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="置顶" style="@style/nk"/>
        <Button android:id="@+id/izo" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="取消置顶" style="@style/nk"/>
        <Button android:id="@+id/bss" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="免打扰" style="@style/nk"/>
        <Button android:id="@+id/aie" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:text="取消免打扰" style="@style/nk"/>
        <com.tencent.mm.ui.widget.MMEditText android:id="@+id/dh2" android:layout_width="match_parent" android:layout_height="45dp" android:layout_marginTop="20dp" android:hint="输入聊天信息"/>
        <Button android:id="@+id/hko" android:background="@color/afp" android:layout_width="match_parent" android:layout_height="45dp" android:text="发送" style="@style/nk"/>
        <TextView android:textSize="20dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="20dp" android:text="接收到的消息xml,如下："/>
        <TextView android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </LinearLayout>
</ScrollView>
